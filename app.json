{
  "expo": {
    "name": "AppCMS Demo",
    "slug": "appcms-demo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "appcms",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0f1c"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cloudhost.demo",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This app uses tracking to provide personalized ads and measure ad performance.",
        "FacebookAppID": "1485851395848055",
        "FacebookClientToken": "70fad7cf550a35c955e8dcc61fb0d07e",
        "FacebookAutoLogAppEventsEnabled": true,
        "FacebookAdvertiserIDCollectionEnabled": true,
        "FacebookDisplayName": "AppCMS Demo",
        "SKAdNetworkItems": [
          { "SKAdNetworkIdentifier": "v9wttpbfk9.skadnetwork" },
          { "SKAdNetworkIdentifier": "n38lu8286q.skadnetwork" }
        ],
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["fb1485851395848055"]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0a0f1c"
      },
      "package": "com.cloudhost.demo",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "@react-native-firebase/app",
      [
        "expo-tracking-transparency",
        {
          "userTrackingPermission": "This app uses tracking to provide personalized ads and measure ad performance."
        }
      ],
      [
        "react-native-fbsdk-next",
        {
          "appID": "1485851395848055",
          "clientToken": "70fad7cf550a35c955e8dcc61fb0d07e",
          "displayName": "AppCMS Demo",
          "advertiserIDCollectionEnabled": true,
          "autoLogAppEventsEnabled": true
        }
      ]
    ]
  }
}
